class Solution {
public:
    int maxSubArray(vector<int>& nums) {
       int currsum=0;
       int n=nums.size();
       int maxmsum=INT_MIN;
       for(int i=0;i<n;i++){
        currsum+=nums[i];
        maxmsum=max(currsum, maxmsum);
       if(currsum<0)
        currsum=0;
       }
       return maxmsum;
    }
};
